{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug"
    }
  },
  "application": {
    "id": "rocket-indexer.testing",
    "name": "Rocket Indexer",
    "description": "Rocket documents indexer",
    "tags": [
      "rocket",
      "csharp",
      "gke",
      "tools"
    ],
    "httpHealthCheck": {
      "portNumber": 8080,
      "url": "/health-check"
    },
    "ipRequestsRateLimiting": {
      "EnableEndpointRateLimiting": true,
      "StackBlockedRequests": false,
      "HttpStatusCode": 429,
      "EndpointWhitelist": [
        "*:/docs"
      ],
      "QuotaExceededResponse": {
        "Content": "{{ \"message\": \"Whoa! Calm down, cowboy!\", \"details\": \"Quota exceeded. Maximum allowed: {0} per {1}. Please try again in {2} second(s).\" }}",
        "ContentType": "application/json",
        "StatusCode": 429
      },
      "GeneralRules": [
        {
          "Endpoint": "*",
          "Period": "1s",
          "Limit": 10
        }
      ]
    }
  },
  "indexerStreamSettings": {
    "exchangeName": "db-events", 
    "queueName": "db-events.indexer",
    "routingKeys": [
      "*.*.Insert",
      "*.*.Update",
      "*.*.Replace",
      "*.*.Delete"
    ],
    "autoDeleteQueue": false,
    "isDurable": true,
    "autoAck": true,
    "arguments": {
      "x-queue-mode": "lazy"
    }
  },
  "mapperStreamSettings": {
    "exchangeName": "db-events",
    "queueName": "db-events.mapper",
    "routingKeys": [
      "*._objects.Insert",
      "*._objects.Update",
      "*._objects.Replace"
    ],
    "autoDeleteQueue": false,
    "isDurable": true,
    "autoAck": true,
    "arguments": {
      "x-queue-mode": "lazy"
    }
  },
  "searchCacheDbContext": {
    "id": "kit-kat",
    "hostName": "kit-kat",
    "portNumber": 9200,
    "proxyHeaders": {
      "X-API-Token": "my-token"
    }
  },
  "messageBrokerServiceContext": {
    "id": "the-rabbit",
    "hostName": "the-rabbit",
    "portNumber": 5672,
    "virtualHost": "",
    "userName": "sa",
    "password": "my-pwd"
  },
  "loggingServiceContext": {
    "id": "logy",
    "baseEndpoint": "http://n8n:5678/webhook/no-api/logy",
    "headers": {
      "X-Access-Token": "1+1===3"
    }
  },
  "zooServiceContext": {
    "id": "zookeeper",
    "baseEndpoint": "http://n8n:5678/webhook/no-api/zookeeper",
    "headers": {
      "X-Access-Token": "1+1===3"
    }
  }
}
